  "email": "johndoe@gmail.com",
  "password": "password123"
}


####################################
###            LOGIN             ###
####################################
POST {{API}}/auth/login
Content-Type: application/json

{
  "email": "johndoe@gmail.com",
  "password": "password123"
}


####################################
###     REFRESH CREDENTIALS      ###
####################################
### * access: PRIVATE            ###
####################################
GET {{API}}/auth/refresh
Authorization: Bearer {{REFRESH_TOKEN}}


####################################
###            LOGOUT            ###
####################################
### * access: PRIVATE            ###
####################################
GET {{API}}/auth/logout
Authorization: Bearer {{ACCESS_TOKEN}}


####################################
